    "description": "Bundle to show statistics over a project ecommerce with sylius",
    "keywords": ["report", "statistics", "opositatest", "symfony", "ecommerce", "sylius"],
    "homepage": "http://opositatest.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Odiseo Team",
            "homepage": "http://odiseo.com.ar"
        }
    ],
    "require": {
        "php": "^7.1",

        "sylius/sylius": "^1.0",
        "odiseoteam/odiseo-util-bundle": "^1.0",
        "odiseoteam/sylius-report-bundle": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "symfony/form": "^3.2",
        "symfony/validator": "^3.2"
